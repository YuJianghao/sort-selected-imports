{
  "name": "sort-selected-imports",
  "displayName": "sort-selected-imports",
  "description": "Sort selected es6 imports for js or ts",
  "version": "0.0.3",
  "publisher": "winwin2011",
  "repository": {
    "url": "https://github.com/YuJianghao/sort-selected-imports"
  },
  "engines": {
    "vscode": "^1.63.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:sort-selected-imports.sort"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "sort-selected-imports.sort",
        "title": "Sort Selected"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "version": "standard-version"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "standard-version": "^9.3.2"
  }
}
